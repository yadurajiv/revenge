<?xml version="1.0" encoding="utf-8"?>
<!-- play around and compile using script in project root :D -->
<flex-config>
  <target-player>11.5</target-player>
  <compiler>
    <define append="true">
      <name>CONFIG::debug</name>
      <value>true</value>
    </define>
    <define append="true">
      <name>CONFIG::release</name>
      <value>false</value>
    </define>
    <define append="true">
      <name>CONFIG::timeStamp</name>
      <value>'16-03-2011'</value>
    </define>
    <source-path append="true">
      <path-element>src</path-element>
    </source-path>
  </compiler>
  <file-specs>
    <path-element>src/Main.as</path-element>
  </file-specs>
  <default-background-color>#FFFFFF</default-background-color>
  <default-frame-rate>30</default-frame-rate>
  <default-size>
    <width>640</width>
    <height>480</height>
  </default-size>
</flex-config>
